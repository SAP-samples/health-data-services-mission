{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/resources/sap/fhir(.*)$",
      "target": "/resources/sap/fhir$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/core(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "coreservice",
      "httpMethods": [
        "POST",
        "OPTIONS",
        "GET"
      ]
    },
    {
      "source": "/core/(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "coreservice",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/meta(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "conformanceservice",
      "httpMethods": [
        "POST",
        "OPTIONS",
        "GET"
      ]
    },
    {
      "source": "/meta/(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "conformanceservice",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/rule(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "rulesservice",
      "httpMethods": [
        "POST",
        "OPTIONS",
        "GET"
      ]
    },
    {
      "source": "/rule/(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "rulesservice",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}