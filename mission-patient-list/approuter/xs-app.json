{
  "authenticationMethod": "route",
  "routes": [  	    
    {
      "source": "^/core(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "coreservice",
      "httpMethods": [
        "POST",
        "OPTIONS",
        "GET"
      ]
    },
    {
      "source": "/core/(.*)$",
      "target": "$1",
      "service": "com.sap.health.fs",
      "endpoint": "coreservice",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(/applications/.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ],
  "welcomeFile": "/com.sap.health.example.mission.patientlist"
}
